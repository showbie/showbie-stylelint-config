{
  "name": "@showbie/stylelint-config",
  "version": "2.0.1",
  "description": "Stylelint configuration for Showbie projects",
  "keywords": [
    "showbie",
    "stylelint-config"
  ],
  "homepage": "https://github.com/showbie/showbie-stylelint-config",
  "repository": "showbie/showbie-stylelint-config",
  "license": "ISC",
  "author": "Showbie (https://www.showbie.com)",
  "maintainers": [
    "Jeff Nelson @stormwarning (https://www.showbie.com)"
  ],
  "scripts": {
    "changelog": "lerna-changelog",
    "changeset": "changeset add",
    "lint": "eslint '**/*.js'",
    "prepare": "husky install",
    "release": "changeset publish",
    "test": "npm run lint"
  },
  "lint-staged": {
    "*.js": "eslint --fix",
    "package.json": "prettier --write"
  },
  "prettier": {
    "singleQuote": true
  },
  "dependencies": {
    "postcss-scss": "^4.0.9",
    "stylelint-config-property-sort-order-smacss": "^10.0.0",
    "stylelint-config-sass-guidelines": "^10.0.0",
    "stylelint-config-standard": "^33.0.0",
    "stylelint-order": "^6.0.4",
    "stylelint-prettier": "^3.0.0",
    "stylelint-scss": "^5.3.2"
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.4.0",
    "@changesets/cli": "^2.27.1",
    "@showbie/eslint-config": "^4.0.0",
    "eslint": "^8.56.0",
    "husky": "^7.0.0",
    "lerna-changelog": "^2.2.0",
    "lint-staged": "^15.2.0",
    "prettier": "^2.8.8",
    "prettier-plugin-packagejson": "^2.4.7",
    "stylelint": "^15.6.3"
  },
  "peerDependencies": {
    "stylelint": ">=15"
  },
  "changelog": {
    "repo": "showbie/showbie-stylelint-config",
    "labels": {
      "breaking": "💣 Breaking Change",
      "removed": "🗑️ Removed",
      "deprecated": "🚚 Deprecated",
      "changed": "♻️ Changed",
      "added": "🎁 Added",
      "fixed": "🐛 Fixed",
      "security": "🔒 Security"
    }
  }
}
